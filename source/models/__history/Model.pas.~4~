unit Model;

interface

type
  ModelNameAttribute = class(TCustomAttribute)
  private
    FModelName: string;
  public
    constructor Create(const AModelName: string); overload;

    property ModelName: string read FModelName write FModelName;
  end;

implementation

{ ModelAttribute }

constructor ModelNameAttribute.Create(const AModelName: string);
begin
  FModelName := AModelName;
end;

end.
